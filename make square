#include <iostream>

using namespace std;

int n, m;

void square(int n, int m)
{
    for(int i = 0 ; i < n ; i ++)
    {   
        
        for(int i = 0 ; i < m ; i++)
        {
            cout << "1";
        }
        cout << endl;
    }
}

int main() {
    cin >> n >> m;

    square(n,m);

    return 0;
}
