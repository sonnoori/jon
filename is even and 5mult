#include <iostream>
#include <string>
using namespace std;

int n;

string iseven(int a)
{
    if(a % 2 == 0)
    {
        if(((a / 10 ) + (a %10)) % 5 == 0 )
        {
            return "Yes";
        }
    }
    else
    return "No";
}

int main() {
    cin >> n;
    cout << iseven(n);

    

    return 0;
}
